<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anacristinapg.proyectotfg" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Interfaz.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Interfaz.LocalizationActivity"
            android:label="@string/title_activity_localization" >
        </activity>

        <service
            android:name="com.example.anacristinapg.proyectotfg.ServiceMain"
            android:enabled="true"
            android:exported="true" >
        </service>

        <activity
            android:name=".Interfaz.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.example.anacristinapg.proyectotfg.Prueba"
            android:label="@string/title_activity_prueba" >
        </activity>
    </application>

</manifest>
